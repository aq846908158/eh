<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.lcvc.POJO.ProductType" table="productType" schema="eh">
        <id name="id" column="id"/>
        <property name="productTypeName" column="productTypeName"/>
        <property name="productTypeRank" column="productTypeRank"/>
        <property name="productTypeCode" column="productTypeCode"/>
        <property name="superType" column="superType"/>
    </class>
</hibernate-mapping>