<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.lcvc.POJO.Order" table="order" schema="eh">
        <id name="id" column="id"/>
        <property name="orderCode" column="orderCode"/>
        <property name="number" column="number"/>
        <property name="sellUser" column="sellUser"/>
        <property name="buyUser" column="buyUser"/>
        <property name="orderPrice" column="orderPrice"/>
        <property name="chalkUp" column="chalkUp"/>
        <property name="createTime" column="createTime"/>
        <property name="message" column="message"/>
    </class>
</hibernate-mapping>